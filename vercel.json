{
	"builds": [
		{
			"src": "server/index.js",
			"use": "@now/node-server"
		}
	],

	"routes": [
		{
			"headers": {
				"Access-Control-Allow-Origin": "*",
				"Access-Control-Allow-Methods": "GET, OPTIONS",
				"Access-Control-Allow-Headers": "X-Requested-With, Content-Type, Accept"
			},
			"src": "/.*",
			"dest": "/server/index.js"
		}
	]
}
